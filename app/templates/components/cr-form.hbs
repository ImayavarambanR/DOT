
<form class="container">
  <div class="form-group">
    <label for="name">Name</label>
    {{input type="text" class="form-control" id="name" value=name placeholder="Enter Name"}}
  </div>
  <div class="form-group">
    <label for="number">Number</label>
    {{input type="number" class="form-control" id="number" value=phone placeholder="Contact Number"}}
  </div>
  <div class="form-group">
    <label for="facebook">Facebook</label>
    {{input type="text" class="form-control" id="facebook" value=facebook placeholder="Profile URL"}}
  </div>
  <div class="form-group">
    <label for="twitter">Twitter</label>
    {{input type="text" class="form-control" id="twitter" value=twitter placeholder="Twitter URL"}}
  </div>
  <div class="form-group">
    <label for="email">Email</label>
    {{input type="email" class="form-control" id="email" value=email placeholder="Email id"}}
  </div>
  <div class="form-group">
    <label for="desc">Description</label>
    {{textarea class="form-control" id="desc" value=desc rows="3"}}
  </div>
  <fieldset class="form-group">
    <legend>Star</legend>
    <div class="form-check">
      <label class="form-check-label">
        <input type="radio" class="form-check-input" name="group" id="op1" value="family" checked>
        5
      </label>
    </div>
    <div class="form-check">
    <label class="form-check-label">
        <input type="radio" class="form-check-input" name="group" id="op2" value="family">
        4
      </label>
    </div>
    <div class="form-check disabled">
    <label class="form-check-label">
        <input type="radio" class="form-check-input" name="group" id="op3" value="family">
        3
      </label>
    </div>
    <div class="form-check disabled">
    <label class="form-check-label">
        <input type="radio" class="form-check-input" name="group" id="op3" value="family">
        2
      </label>
    </div>
    <div class="form-check disabled">
    <label class="form-check-label">
        <input type="radio" class="form-check-input" name="group" id="op3" value="family">
        1
      </label>
    </div>
  </fieldset>
  {{#link-to 'prop.index'}}<button type="submit" class="btn btn-primary" {{action "clickyy"}}>OK :P</button>{{/link-to}}
</form>
{{#if issad}}
  {{yield con}}
{{/if}}

{{yield}}
